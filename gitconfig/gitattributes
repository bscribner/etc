.gitattributes	export-ignore
.gitignore	export-ignore

*.txt           text
*.c		text
*.cpp		text
*.h		text
*.hpp		text
*.C		text
*.rst		text

*.py		text diff=python
*.tex		text diff=tex
*.java		text diff=java
*.pl		text diff=perl
*.php		text diff=php
*.rb		text diff=ruby

*.vcproj        eol=crlf
*.dsp		eol=crlf
*.dsw		eol=crlf

*.sh            eol=lf

*.jpg           binary
*.png		binary
*.gif		binary
